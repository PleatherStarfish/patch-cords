{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "env":
  {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules":
  {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "implicit-arrow-linebreak": "off",
    "comma-dangle": "off",
    "indent": "off",
    "no-trailing-spaces": "off",
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "no-console":  0
  }
}